<% provide(:title, 'All musics') %>
  <% form_tag music_index_path, :method => 'get' do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search", :name => nil %>
  </p>
<section>
  <% end %>
    <ul class="music">
      <% @music.each do |music| %>
        <li>
        <%= link_to music.artist, music %> -
        <%= link_to music.songname, music %>
        </li>
      <% end %>
    </ul>
</section>